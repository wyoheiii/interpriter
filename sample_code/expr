var a = "global a";
var b = "global b";
var c = "global c";

{
  var a = "outer a";
  var b = "outer b";
  {
    var a = "innner a";
    print a;
    print b;
    print c;
  }
  print a;
  print b;
  print c;
}
print a;
print b;
print c;


print nil and nil;
print nil or "b";


var a = 1;

while (a < 3) {
  print a;
  a = a + 1;
}